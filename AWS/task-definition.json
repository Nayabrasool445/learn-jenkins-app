{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "127552426662.dkr.ecr.us-east-2.amazonaws.com/mystaticwebapp:latest",
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::127552426662:role/ecsTaskExecutionRole"
}