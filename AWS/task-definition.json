{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "staticwebapp",
            "image": "nayabrasool445/my-ecs-images:staticwebapp",
            "repositoryCredentials": {
                "credentialsParameter": "arn:aws:secretsmanager:us-east-2:127552426662:secret:Credentials/dockerhub-bDC1Hh"
              },
            "portMappings": [
                {
                    "name": "ngnix-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::127552426662:role/ecsTaskExecutionRole"
}