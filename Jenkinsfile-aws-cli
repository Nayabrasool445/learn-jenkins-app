pipeline {
    agent any

    stages {
        stage('Building Images') {
            agent{
                docker{
                    image 'centos'
                    reuseNode true
                }
            }
            steps {
                sh '''
                   docker --version
                   docker build -f AWS/Dockerfile-aws-cli -t my-aws-cli . 
                '''
            }
        }
    }
}

